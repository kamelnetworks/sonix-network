[{
  "selector": "node",
  "style": {
    "text-valign": "center",
    "text-halign": "left"
  }
}, {
  "selector": "node",
  "style": {
    "shape": "round-triangle",
    "background-color": "#5cbef7"
  }
}, {
  "selector": "node[?flipLabel]",
  "style": {
    "text-halign": "right"
  }
}, {
  "selector": "node[type]",
  "style": {
    "label": "data(type)"
  }
}, {
  "selector": "edge",
  "style": {
    "width": 1
  }
}, {
  "selector": "edge.bezier",
  "style": {
    "curve-style": "bezier",
    "control-point-step-size": 40
  }
}]
